$TTL    86400
@       IN      SOA     dns.mysite.lan. admin.mysite.lan. (
                2024010101 ; Serial (année/mois/jour/numéro de révision)
                7200       ; Refresh (2 heures)
                3600       ; Retry (1 heure)
                1209600    ; Expire (14 jours)
                86400      ; Negative Cache TTL (1 jour)
)

        IN      NS      dns.mysite.lan.   ; Serveur de noms principal

dns_ad  IN      A      192.168.0.2
